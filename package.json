{
  "name": "global-prefix",
  "description": "Get the npm global path prefix.",
  "version": "0.1.2",
  "homepage": "https://github.com/jonschlinkert/global-prefix",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
